<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.gt.entrypad">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <application
        android:requestLegacyExternalStorage="true">
        <meta-data
            android:name="com.gt.entrypad.api.ApiConfigModule"
            android:value="ConfigModule" />
    <activity android:name=".module.project.ui.activity.ProjectListActivity"
        android:screenOrientation="landscape"
        android:theme="@style/MainAppTheme"
        android:windowSoftInputMode="adjustResize|stateHidden"/>

        <activity android:name="com.zx.zxutils.views.PhotoPicker.PhotoPickerActivity"
            android:theme="@style/MainAppTheme"
            android:screenOrientation="landscape"
            />
        <activity android:name="com.zx.zxutils.views.PhotoPicker.PhotoPagerActivity"
            android:theme="@style/MainAppTheme"
            android:screenOrientation="landscape"/>
        <activity android:name=".module.project.ui.activity.ScanIdCardActivity"
            android:screenOrientation="landscape"
            android:theme="@style/MainAppTheme"/>
        <activity android:name=".module.project.ui.activity.InfoInputActivity"
            android:screenOrientation="landscape"
            android:theme="@style/MainAppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden"/>
        <activity android:name=".module.project.ui.activity.TakePhotoActivity"
            android:screenOrientation="landscape"
            android:theme="@style/MainAppTheme"/>
        <activity android:name=".module.project.ui.activity.GroundFigureActivity"
            android:screenOrientation="landscape"
            android:theme="@style/MainAppTheme"/>
        <activity android:name=".module.project.ui.activity.ResultShowActivity"
            android:screenOrientation="landscape"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:theme="@style/MainAppTheme"/>
        <activity android:name=".module.project.ui.activity.DrawSketchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/MainAppTheme"/>

        <service
            android:name="com.tencent.smtt.export.external.DexClassLoaderProviderService"
            android:label="dexopt"
            android:process=":dexopt" >
        </service>
    </application>
</manifest>